<!DOCTYPE html>
<html>
  <head>
    <title>Ruby on Rails Tutorial Sample App | <%= @title %></title>
    <%= csrf_meta_tag %>
	<%= javascript_include_tag :defaults %>
	<%= render 'layouts/stylesheets' %>
  </head>
  <body>
  
	<%= render 'layouts/header' %>
	<section>
		<% flash.each do |key,value| %>
		<div class="flash #{key}"><%= value %></div>
		<% end %> 
		<%= yield %>
	</section>
	
  </body>
</html>
